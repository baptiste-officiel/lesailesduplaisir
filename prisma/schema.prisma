// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  status String? 
  aeroclubName String? 
  aeroclubAddress String? 
  email String  @unique
  hashedPassword String
  name  String
}

model Plane {
  id    Int     @id @default(autoincrement())
  name  String
  img  String
  seats String? 
  vmax String? 
  weight String? 
}